<project default="create_run_jar" name="Create Runnable Jar for Project Port Scanner">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="./PortScanner.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="de.unipotsdam.hpi.PortScanner"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="/Users/tom/Documents/Programming/Java/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="./args4j-2.0.21.jar"/>
        </jar>
    </target>
</project>

